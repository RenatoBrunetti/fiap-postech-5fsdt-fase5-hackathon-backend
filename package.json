{
  "name": "fiap-postech-5fsdt-fase5-hackathon-backend",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "private": true,
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "tsx --watch --env-file .env src/server.ts",
    "start": "node --env-file .env dist/server.js",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "tsc --noEmit && eslint .",
    "lint:fix": "tsc --noEmit && eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --passWithNoTests --runInBand --detectOpenHandles --silent --noStackTrace",
    "test:coverage": "jest --coverage",
    "test:debug": "jest --verbose --silent=false --noStackTrace=false --detectOpenHandles",
    "test:staged": "npm run test -- --onlyChanged --changedSince=main",
    "test:ci": "npm run test -- --forceExit",
    "test:watch": "npm run test:debug -- --watch"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@jest/globals": "30.2.0",
    "@types/bcryptjs": "2.4.6",
    "@types/cookie-parser": "1.4.10",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.6",
    "@types/jest": "30.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "25.0.10",
    "@types/pg": "8.16.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "29.12.1",
    "eslint-plugin-prettier": "5.5.5",
    "jest": "30.2.0",
    "prettier": "3.8.1",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.1"
  },
  "dependencies": {
    "bcryptjs": "3.0.3",
    "cookie-parser": "1.4.7",
    "cors": "2.8.6",
    "dotenv": "17.2.3",
    "express": "5.2.1",
    "helmet": "8.1.0",
    "jsonwebtoken": "9.0.3",
    "pg": "8.17.2",
    "reflect-metadata": "0.2.2",
    "typeorm": "0.3.28",
    "zod": "4.3.6"
  }
}
